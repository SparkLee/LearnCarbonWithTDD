{
    "name": "sparklee/learn-carbon-with-tdd",
    "type": "project",
    "description": "以 TDD 的方式学习 Carbon 时间对象组件",
    "keywords": [
        "TDD",
        "PHPUnit",
        "Carbon"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Spark Lee",
            "email": "liweijsj@163.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "nesbot/carbon": "2.28"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5||^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
